<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tarena.entity.Question"
	table="t_question">
		<id name="qid" type="integer" column="qid">
			<generator class="identity"></generator>
		</id>
		<property name="content" type="string" column="content"></property>
		<property name="answer" type="string" column="answer"></property>
		<property name="type" type="string" column="type"></property>
		<property name="examPaperId" type="integer" column="examPaper_id"></property>
		
	</class>
</hibernate-mapping>